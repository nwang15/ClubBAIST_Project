@page
@model GolfBAIST_Project.Pages.Member.ReviewReservationModel
@{
}



<div class="col-md-4">
    <input type="text" class="form-control" id="userId" value="@Model.memberId" readonly>
</div>


<div class="container">
    <table class="table">
        <thead>
            <tr>
                <td>Reservation Id</td>
                <td>Reservation Time</td>
                <td>Players</td>
                <td></td>
            </tr>
        </thead>
        <tbody>
            @foreach (var reservation in Model.reservations)
            {
                <tr>
                    <td>@reservation.ReservationId</td>
                    <td>@reservation.TimeRange</td>
                    <td>@reservation.Players</td>
                    <td><a href="/Member/ReservationInfo/@reservation.ReservationId">View</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>
